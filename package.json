{
  "name": "angular-templates",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start:ecommerce": "ng serve ecommerce",
    "start:dashboard": "ng serve dashboard",
    "build:ecommerce": "ng build ecommerce",
    "build:dashboard": "ng build dashboard",
    "build:shared": "ng build shared",
    "watch:ecommerce": "ng build ecommerce --watch --configuration development",
    "watch:shared": "ng build shared --watch",
    "lint:ecommerce": "ng lint ecommerce",
    "build-prod:ecommerce": "ng build ecommerce --configuration production",
    "serve:ssr:ecommerce": "node dist/ecommerce/server/server.mjs",
    "test:ecommerce": "ng test ecommerce",
    "test:shared": "ng test shared",
    "e2e:ecommerce": "ng e2e ecommerce",
    "e2e-watch:ecommerce": "nodemon --ext ts --watch 'projects/ecommerce/e2e/tests/**/*.e2e.ts' --exec npm run e2e:ecommerce --signal SIGTERM",
    "serve:ssr:dashboard": "node dist/dashboard/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.2",
    "@angular/common": "^18.0.2",
    "@angular/compiler": "^18.0.2",
    "@angular/core": "^18.0.2",
    "@angular/forms": "^18.0.2",
    "@angular/platform-browser": "^18.0.2",
    "@angular/platform-browser-dynamic": "^18.0.2",
    "@angular/platform-server": "^18.0.2",
    "@angular/router": "^18.0.2",
    "@angular/ssr": "^18.0.3",
    "express": "^4.18.2",
    "immutable": "^4.3.5",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.3",
    "@angular-eslint/builder": "18.0.1",
    "@angular-eslint/eslint-plugin": "18.0.1",
    "@angular-eslint/eslint-plugin-template": "18.0.1",
    "@angular-eslint/schematics": "18.0.1",
    "@angular-eslint/template-parser": "18.0.1",
    "@angular/cli": "^18.0.3",
    "@angular/compiler-cli": "^18.0.2",
    "@puppeteer/ng-schematics": "^0.6.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "jasmine": "5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^18.0.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "puppeteer": "22.6.3",
    "typescript": "~5.4.5"
  }
